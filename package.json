{
  "name": "fpv-drone.info",
  "description": "News, Guides, Reviews and more for the FPV Community",
  "engines": {
    "node": "^20.10.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx && npm run build.sw",
    "build.server": "vite build -c adapters/static/vite.config.ts && npm run build.sw",
    "build.sw": "npx workbox-cli injectManifest ./workbox-config.cjs",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr --host",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "git.clean": "git branch --merged | grep -Ev \"(^\\*|master|main|dev)\" | xargs git branch -d",
    "icon_pwa:update": "npx tsx ./scripts/icon_and_pwa.ts",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "upgrade-packages": "npx npm-check-updates --interactive",
    "vercell-install": "npm ci && npm run i18n:download",
    "qwik": "qwik",
    "i18n:download": "npx tsx ./scripts/i18n_download.ts"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.2",
    "@builder.io/qwik-city": "^1.5.2",
    "@types/eslint": "^8.56.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.22",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.2",
    "prettier": "^3.2.5",
    "qwik-speak": "^0.22.0",
    "typescript": "5.4.5",
    "undici": "*",
    "vercel": "^34.0.0",
    "vite": "^5.2.9",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@builder.io/qwik-worker": "^0.0.1",
    "@fontsource-variable/montserrat": "^5.0.18",
    "@quasarwork/qwik-city-nprogress": "^0.0.3",
    "@storyblok/js": "^3.0.8",
    "@uav.painkillers/pid-analyzer-wasm": "^3.2.2",
    "@vercel/analytics": "^1.2.2",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.5.1",
    "drag-drop": "^7.2.0",
    "echarts": "^5.5.0",
    "lodash-es": "^4.17.21",
    "npm": "^10.5.2",
    "qwik-transition": "^0.0.7",
    "workbox-recipes": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  }
}
